{
    "name": "yulhighlight",
    "displayName": "Yul Highlight",
    "description": "Code highlighting for Yul Language EVM dialect",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.76.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "yul",
                "aliases": [
                    "Yul",
                    "yul"
                ],
                "extensions": [
                    ".yul"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "yul",
                "scopeName": "source.yul",
                "path": "./syntaxes/yul.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zzh1996/vscode-yul-highlight.git"
    },
    "publisher": "zzh1996"
}
