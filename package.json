{
    "name": "yulhighlight",
    "displayName": "YulHighlight",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "yul",
                "aliases": [
                    "Yul",
                    "yul"
                ],
                "extensions": [
                    ".yul"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "yul",
                "scopeName": "source.yul",
                "path": "./syntaxes/yul.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
